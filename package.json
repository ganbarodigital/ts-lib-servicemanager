  "dependencies": {
    "@ganbarodigital/ts-lib-error-reporting": "^0.2.1",
    "@ganbarodigital/ts-lib-http-types": "^0.1.5",
    "@ganbarodigital/ts-lib-packagename": "^0.2.3",
    "rfdc": "^1.1.4"
  },
  "devDependencies": {
    "typescript": "^3.8.0",
    "tslint": "^5.20.1",
    "@types/node": "^13.11.0",
    "@types/rfdc": "^1.1.0",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.1",
